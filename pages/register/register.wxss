/*注册*/
.register{
  margin-top:400rpx;
}
.register .info{
  width:90%;
  height: 100rpx;
  margin:auto;
  border-bottom: 1rpx solid #d6d6d6;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.info-left{
  display: flex;
  align-items: center;
}
.info-img{
  width:80rpx;
  height: 80rpx;
  display:block;
  border-radius: 50%;
  margin-right:20rpx;
}
.info-name{
  font-size:26rpx;
}
.btn{
  width:160rpx !important;
  height:40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background:#f19dad;
  color:#fff;
  font-size:26rpx;
  padding:0rpx;
  margin:0rpx !important;
}
.registerBtn{
  width:90% !important;
  margin:60rpx auto;
  background:#f19dad;
  color:#fff;
}
