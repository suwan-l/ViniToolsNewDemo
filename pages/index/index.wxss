/*首页*/
page{
  width: 750rpx;
  background: #f6f6f6;
}
.swiper{
  width: 100%;
  height: 400rpx;
  background: #000;
}
.swiper-conn{
  width: 100%;
  height: 100%;
}
.swiper-conn swiper-item image{
  width:100%;
  height:100%;
}

.conn{
  width: 100%;
  margin:20rpx auto;
  background: #fff;
  border-radius: 10rpx;
}
.title{
  width:100%;
  height: 100rpx;
  display:flex;
  align-items: center;
  text-indent: 1em;
  border-bottom: 1rpx solid #f6f6f6;
}
.list{
  width:100%;
  display: flex;
  flex-wrap: wrap;
  align-items: flex-start;
  align-content: flex-start;
  justify-content:start;
}
.list .listLi{
  width:calc(100%/3);
  background:#fff;
  border-left:1rpx solid #f6f6f6; 
  border-bottom:1rpx solid #f6f6f6; 
  border-right:1rpx solid #f6f6f6; 
  box-sizing:border-box;
}
.item{
  display: flex;
  flex-direction:column; 
  align-items: center;
  justify-items: center;
}
.listImg{
  width:100%;
  display:block;
}
.listLi image{
  width: 60rpx;
  height: 60rpx;
  display:block;
  margin:20rpx auto 0rpx;
}
.listLi text{
  width: 100%;
  text-align:center;
  display:block;
  font-size: 26rpx;
  margin:10rpx auto 20rpx;
}
